{
  "name": "quick-start",
  "version": "0.4.0",
  "description": "Project creator for node.js/javascript applications",
  "main": "./lib/index.js",
  "dependencies": {
    "glob": "^5.0.15",
    "homedir": "^0.6.0",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1",
    "wrench": "^1.5.8"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-jest": "^9.0.3",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "jest-cli": "^0.9.2"
  },
  "jest": {
    "automock": false,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/test-constants.js",
      "/__tests__/test-utils.js"
    ]
  },
  "scripts": {
    "compile": "babel -d lib/ src/",
    "lint": "eslint src/",
    "prepublish": "npm run lint && npm run compile",
    "pretest": "./scripts/pretest.sh",
    "test": "jest",
    "posttest": "./scripts/posttest.sh",
    "postinstall": "./scripts/postinstall.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kasperekt/quick-start"
  },
  "bugs": {
    "url": "https://github.com/kasperekt/quick-start/issues"
  },
  "preferGlobal": true,
  "author": "Tomasz Kasperek <tomasz.kasperek@icloud.com> (http://tomaszkasperek.me)",
  "license": "MIT",
  "bin": "./bin/qs",
  "keywords": [
    "scaffold",
    "quick-start",
    "quickstart",
    "creator",
    "project-creator"
  ]
}
